#Subgroup analysis based on ethnicity
test_hispanic <- test[test$ETHNICITY_HISPANIC==1, ]
test_nonhispanic <- test[test$ETHNICITY_HISPANIC==0, ]
predictors_test_hispanic <- test_hispanic %>% select(AdmQtr, OperYR, Age, SEX, BMI, RACE_NEW, ETHNICITY_HISPANIC, ORIGIN, HYPERMED, DIABETES, SMOKE_CURRENT, HXCHF, HXCOPD, DIALYSIS, FNSTATUS2, ASACLAS, PRIOR_OSR, PRIOR_EVAR, PRIOR_ABDO, PRSODM_mmolL, PRBUN_mmolL, PRCREAT_umolL, PRALBUM_gL, PRWBC_Kmm3, PRHCT_LL, PRPLATE_Kcumm, PRINR, PRPTT_s, AAA_DIAM, DIST_EXT, MAIN_BODY, ACCESS, ILIAC_CONDUIT, IIA_EMBO, IIA_REVASC, IBG)
predictors_test_nonhispanic <- test_nonhispanic %>% select(AdmQtr, OperYR, Age, SEX, BMI, RACE_NEW, ETHNICITY_HISPANIC, ORIGIN, HYPERMED, DIABETES, SMOKE_CURRENT, HXCHF, HXCOPD, DIALYSIS, FNSTATUS2, ASACLAS, PRIOR_OSR, PRIOR_EVAR, PRIOR_ABDO, PRSODM_mmolL, PRBUN_mmolL, PRCREAT_umolL, PRALBUM_gL, PRWBC_Kmm3, PRHCT_LL, PRPLATE_Kcumm, PRINR, PRPTT_s, AAA_DIAM, DIST_EXT, MAIN_BODY, ACCESS, ILIAC_CONDUIT, IIA_EMBO, IIA_REVASC, IBG)

pred_hispanic <- predict(XGB, as.matrix(predictors_test_hispanic))
auc(test_hispanic$MACE, pred_hispanic)
ci.auc(test_hispanic$MACE, pred_hispanic)

pred_nonhispanic <- predict(XGB, as.matrix(predictors_test_nonhispanic))
